msgstr "suffix"

#: src/lib/Sympa/Config/Schema.pm:3398 src/lib/Sympa/Config/Schema.pm:3954
msgid "search scope"
msgstr "sökomfång"

#: src/lib/Sympa/Config/Schema.pm:3406 src/lib/Sympa/Config/Schema.pm:3962
