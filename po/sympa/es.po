#: src/lib/Sympa/Config/Schema.pm:4640
msgid "Latest family instantiation"
msgstr "Última instanciación de la familia"

#: src/lib/Sympa/Config/Schema.pm:4645
msgid "who ran the instantiation"
msgstr "quien ejecutó la instanciación"

#: src/lib/Sympa/Config/Schema.pm:4712
msgid "who updated the config"
msgstr "quien actualizó la configuración"

#: src/lib/Sympa/Config/Schema.pm:4768
msgid "URL prefix of web interface"
msgstr "Prefijo de la URL de la interfaz web"

#: src/lib/Sympa/Config/Schema.pm:4769
