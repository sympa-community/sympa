<SUBJECTHEADER>
<TABLE BORDER="0" WIDTH="100%" CELLPADDING="2" CELLSPACING="2">
<TR>
<TD>
<FONT SIZE="+1" COLOR="($tag$% dark_color %$tag$)"><STRONG>$SUBJECTNA$</STRONG></FONT>, &nbsp;<FONT SIZE="+1">$FROMNAME$</FONT>
</TD>

($tag$% IF user.email && action == 'arc' %$tag$)
<TD ALIGN="right" BGCOLOR="($tag$% bg_color %$tag$)">
<FONT SIZE="-1">
<!-- reply to button -->
<FORM ACTION="($tag$% path_cgi %$tag$)" METHOD=POST>
  <INPUT TYPE="hidden" NAME="in_reply_to" VALUE="$MSGID$">
  <INPUT TYPE="hidden" NAME="subject" VALUE="Re: $SUBJECTNA$">
  <INPUT TYPE="hidden" NAME="list" VALUE="($tag$% list %$tag$)">
  ($tag$%|loc%$tag$)Reply to sender($tag$%END%$tag$)
  <INPUT TYPE="radio" NAME="to" VALUE="$FROMADDRNAME$ $FROMADDRDOMAIN$" CHECKED>
  ($tag$%|loc%$tag$)to list($tag$%END%$tag$)
  <INPUT TYPE="radio" NAME="to" VALUE="($tag$% list %$tag$) ($tag$% list_conf.host %$tag$)">
  <INPUT TYPE="submit" NAME="action_compose_mail" VALUE="($tag$%|loc%$tag$)Reply($tag$%END%$tag$)">
  <INPUT TYPE="submit" NAME="action_send_me" VALUE="($tag$%|loc(user.email)%$tag$)send it back to %1($tag$%END%$tag$)">	
  ($tag$% IF action != 'viewmod' %$tag$)
    ($tag$% IF user.email == '$FROMADDR$' %$tag$)
    <INPUT TYPE="hidden" NAME="yyyy" VALUE="$yyyy$">
    <INPUT TYPE="hidden" NAME="month" VALUE="$mois$">
    <INPUT TYPE="hidden" NAME="msgid" VALUE="$MSGID$">
    <INPUT TYPE="hidden" NAME="action" VALUE="">
    <INPUT TYPE="submit" NAME="action_remove_arc" VALUE="($tag$%|loc%$tag$)tag this mail for deletion($tag$%END%$tag$)" onClick="return request_confirm('($tag$%|loc%$tag$)Do you really want to delete this message ?($tag$%END%$tag$)')">

    ($tag$% ELSIF is_owner %$tag$)
    <INPUT TYPE="hidden" NAME="yyyy" VALUE="$yyyy$">
    <INPUT TYPE="hidden" NAME="month" VALUE="$mois$">
    <INPUT TYPE="hidden" NAME="msgid" VALUE="$MSGID$">
    <INPUT TYPE="hidden" NAME="action" VALUE="">
    <INPUT TYPE="submit" NAME="action_remove_arc" VALUE="($tag$%|loc%$tag$)tag this mail for deletion($tag$%END%$tag$)" onClick="return request_confirm('($tag$%|loc%$tag$)Do you really want to delete this message ?($tag$%END%$tag$)')">
    ($tag$% END %$tag$)
  ($tag$% END %$tag$)
</FORM>
</FONT>
</TD>
($tag$% END %$tag$)
</TR>
</TABLE>
<UL>
</SUBJECTHEADER>

