<SubjectHeader>
<TABLE border=0 width=100%>
<TR>
 <TD>
  <FONT size=+1 COLOR="($time$% dark_color %$time$)">
  <B> $SUBJECTNA$</B></FONT><FONT size=+1> $FROMNAME$ </FONT>
 </TD>

<TD ALIGN=RIGHT bgcolor="($time$% bg_color %$time$)">
<FONT size="-1">
($time$% IF user.email && action == 'arc' %$time$)
<!-- reply to button -->
<FORM ACTION="($time$% path_cgi %$time$)" METHOD=POST>
  <INPUT TYPE="hidden" NAME="in_reply_to" VALUE="$MSGID$">
  <INPUT TYPE="hidden" NAME="subject" VALUE="Re: $SUBJECTNA$">
  <INPUT TYPE="hidden" NAME="list" VALUE="($time$% list %$time$)">
($time$%|loc%$time$)Reply to sender($time$%END%$time$)
<INPUT TYPE="radio" NAME="to" VALUE="$FROMADDRNAME$ $FROMADDRDOMAIN$" CHECKED>
($time$%|loc%$time$)to list($time$%END%$time$)
<INPUT TYPE="radio" NAME="to" VALUE="($time$% list %$time$) ($time$% list_conf.host %$time$)">
<INPUT TYPE="submit" NAME="action_compose_mail" VALUE="($time$%|loc%$time$)Reply($time$%END%$time$)">
<INPUT TYPE="submit" NAME="action_send_me" VALUE="($time$%|loc(user.email)%$time$)send it back to %1($time$%END%$time$)">	

($time$% IF action != 'viewmod' %$time$)
($time$% IF user.email == '$FROMADDR$' %$time$)
  <INPUT TYPE="hidden" NAME="yyyy" VALUE="$yyyy$">
  <INPUT TYPE="hidden" NAME="month" VALUE="$mois$">
  <INPUT TYPE="hidden" NAME="msgid" VALUE="$MSGID$">
  <INPUT TYPE="hidden" NAME="action" VALUE="">
  <INPUT TYPE="submit" NAME="action_remove_arc" VALUE="($time$%|loc%$time$)tag this mail for deletion($time$%END%$time$)" onClick="return request_confirm('($time$%|loc%$time$)Do you really want to delete this message ?($time$%END%$time$)')">

($time$% ELSIF is_owner %$time$)
  <INPUT TYPE="hidden" NAME="yyyy" VALUE="$yyyy$">
  <INPUT TYPE="hidden" NAME="month" VALUE="$mois$">
  <INPUT TYPE="hidden" NAME="msgid" VALUE="$MSGID$">
  <INPUT TYPE="hidden" NAME="action" VALUE="">
  <INPUT TYPE="submit" NAME="action_remove_arc" VALUE="($time$%|loc%$time$)tag this mail for deletion($time$%END%$time$)" onClick="return request_confirm('($time$%|loc%$time$)Do you really want to delete this message ?($time$%END%$time$)')">
($time$% END %$time$)
($time$% END %$time$)
</FORM>

($time$% END %$time$)
</FONT>
 </TD>
 </TR>
</TABLE>

</SubjectHeader>

