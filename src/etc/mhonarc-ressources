<WEEKDAYS>
<-%TAGS [% %]%->[%|l%]Sunday:Monday:Tuesday:Wednesday:Thursday:Friday:Saturday[%END%][%TAGS <-% %-> %]
</WEEKDAYS>

<-% IF lang == 'cz' %->
<WEEKDAYSABR>
Ne:Po:Út:St:Èt:Pá:So
</WEEKDAYSABR>
<-% ELSIF lang == 'hu' %->
<WEEKDAYSABR>
V:H:K:Sz:Cs:P:Szo
</WEEKDAYSABR>
<-% ELSIF lang == 'et' %->
<WEEKDAYSABR>
P:M:T:K:N:R:L
</WEEKDAYSABR>
<-% END %->

<MONTHS>
<-%TAGS [% %]%->[%|l%]January:February:March:April:May:June:July:August:September:October:November:December[%END%][%TAGS <-% %-> %]
</MONTHS>

<-% IF lang == 'cz' %->
<MONTHSABR>
Led:Úno:Bøe:Dub:Kvì:Èern:Èerv:Srp:Záø:Øíj:List:Pro
</MONTHSABR>
<-% ELSIF lang == 'hu' %->
<MONTHSABR>
Jan:Feb:Már:Ápr:Máj:Jún:Júl:Aug:Szep:Okt:Nov:Dec
</MONTHSABR>
<-% ELSIF lang == 'hu' %->
<MONTHSABR>
Jan:Veeb:Mär:Apr:Mai:Juun:Juul:Aug:Sep:Okt:Nov:Dets
</MONTHSABR>
<-% END %->

<-% IF lang == 'cz' %->
<UseLocalTime>

<MSGLOCALDATEFMT>
%d. %B. %Y %H:%M
</MSGLOCALDATEFMT>

<LOCALDATEFMT>
%d. %B. %Y %H:%M
</LOCALDATEFMT>
<-% ELSIF lang == 'hu' %->
<UseLocalTime>
 
<MSGLOCALDATEFMT>
%Y. %B %d., %A %H:%M
</MSGLOCALDATEFMT>

<LOCALDATEFMT>
%Y. %B %d., %A %H:%M
</LOCALDATEFMT>

<-% ELSIF lang == 'et' %->
<UseLocalTime>

<MSGLOCALDATEFMT>
%Y. %B %d., %A %H:%M
</MSGLOCALDATEFMT>

<LOCALDATEFMT>
%Y. %B %d., %A %H:%M
</LOCALDATEFMT>
 
<-% ELSE %->
<MsgLocalDateFmt>
%A %d %B %Y
</MsgLocalDateFmt>
<-% END %->

