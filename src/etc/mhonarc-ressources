<SubjectHeader>
<TABLE border=0 width=100%>
<TR>
 <TD>
  <FONT size=+1 COLOR="[dark_color]">
  [STOPPARSE]
  <B> $SUBJECTNA$</B></FONT><FONT size=+1> $FROMNAME$ </FONT>
  [STARTPARSE]
 </TD>

<TD ALIGN=RIGHT bgcolor="[bg_color]">
<FONT size="-1">
[IF user->email]
<!-- reply to button -->
<FORM ACTION="[path_cgi]" METHOD=POST>
  [STOPPARSE]
  <INPUT TYPE="hidden" NAME="in_reply_to" VALUE="$MSGID$">
  <INPUT TYPE="hidden" NAME="subject" VALUE="Re: $SUBJECTNA$">
  [STARTPARSE]
  <INPUT TYPE="hidden" NAME="list" VALUE="[list]">
[IF lang=fr]
   répondre à l'auteur <INPUT TYPE="radio" NAME="to" VALUE="$FROMADDRNAME$ $FROMADDRDOMAIN$" CHECKED>
  à la liste <INPUT TYPE="radio" NAME="to" VALUE="[list] [list_conf->host]">
 <INPUT TYPE="submit" NAME="action_compose_mail" VALUE="Répondre">
  <INPUT TYPE="submit" NAME="action_send_me" VALUE="revoyer à [user->email]">
[ELSIF lang=hu]
  Válasz a feladónak <INPUT TYPE="radio" NAME="to" VALUE="$FROMADDRNAME$ $FROMADDRDOMAIN$" CHECKED>
  a listának <INPUT TYPE="radio" NAME="to" VALUE="[list] [list_conf->host]">
  <INPUT TYPE="submit" NAME="action_compose_mail" VALUE="Válasz">
  <INPUT TYPE="submit" NAME="action_send_me" VALUE="send it back to [user->email]">
[ELSIF lang=et]
  Vasta saatjale <INPUT TYPE="radio" NAME="to" VALUE="$FROMADDRNAME$ $FROMADDRDOMAIN$" CHECKED>
  listi <INPUT TYPE="radio" NAME="to" VALUE="[list] [list_conf->host]">
  <INPUT TYPE="submit" NAME="action_compose_mail" VALUE="Vasta">
  <INPUT TYPE="submit" NAME="action_send_me" VALUE="send it back to [user->email]">
[ELSIF lang=de]
  Antwort an Sender <INPUT TYPE="radio" NAME="to" VALUE="$FROMADDRNAME$ $FROMADDRDOMAIN$" CHECKED>
  an Liste <INPUT TYPE="radio" NAME="to" VALUE="[list] [list_conf->host]">
  <INPUT TYPE="submit" NAME="action_compose_mail" VALUE="Antworten">
  <INPUT TYPE="submit" NAME="action_send_me" VALUE="send it back to [user->email]">
[ELSE] 
  Reply to sender <INPUT TYPE="radio" NAME="to" VALUE="$FROMADDRNAME$ $FROMADDRDOMAIN$" CHECKED>
  to list <INPUT TYPE="radio" NAME="to" VALUE="[list] [list_conf->host]">
  <INPUT TYPE="submit" NAME="action_compose_mail" VALUE="Reply">
  <INPUT TYPE="submit" NAME="action_send_me" VALUE="send it back to [user->email]">	
[ENDIF]

[IF action<>viewmod]
[IF user->email=$FROMADDR$]
  <INPUT TYPE="hidden" NAME="yyyy" VALUE="$yyyy$">
  <INPUT TYPE="hidden" NAME="month" VALUE="$mois$">
  <INPUT TYPE="hidden" NAME="msgid" VALUE="$MSGID$">
  <INPUT TYPE="hidden" NAME="action" VALUE="">
[IF lang=fr]
  <INPUT TYPE="button" NAME="action_remove_arc" VALUE="marquer ce message pour suppression" onClick="request_confirm(this.form,'Voulez-vous vraiment supprimer ce message ?','remove_arc')">
[ELSIF lang=cz]
  <INPUT TYPE="submit" NAME="action_remove_arc" VALUE="oznaèit tuto zprávu ke smazání">
[ELSIF lang=hu]
  <INPUT TYPE="button" NAME="action_remove_arc" VALUE="A levél megjelõlése törlésre" onClick="request_confirm(this.form,'Tényleg törölni szeretnéd ezt a levelet?','remove_arc')">
[ELSIF lang=et]
  <INPUT TYPE="button" NAME="action_remove_arc" VALUE="Märgi see kiri kustutamiseks" onClick="request_confirm(this.form,'Oled kindel, et soovid selle kirja ära kustutada?','remove_arc')">
[ELSIF lang=de]
  <INPUT TYPE="button" NAME="action_remove_arc" VALUE="diese Mitteilung zum Löschen markieren" onClick="request_confirm(this.form,'Wollen Sie wirklich diese Mitteilung löschen?','remove_arc')">
[ELSE]
  <INPUT TYPE="button" NAME="action_remove_arc" VALUE="tag this mail for deletion" onClick="request_confirm(this.form,'Do you really want to delete this message ?','remove_arc')">
[ENDIF]

[ELSIF is_owner]
  <INPUT TYPE="hidden" NAME="yyyy" VALUE="$yyyy$">
  <INPUT TYPE="hidden" NAME="month" VALUE="$mois$">
  <INPUT TYPE="hidden" NAME="msgid" VALUE="$MSGID$">
  <INPUT TYPE="hidden" NAME="action" VALUE="">

[IF lang=fr]
  <INPUT TYPE="button" NAME="action_remove_arc" VALUE="marquer ce message pour suppression" onClick="request_confirm(this.form,'Voulez-vous vraiment supprimer ce message ?','remove_arc')">
[ELSIF lang=cz]
  <INPUT TYPE="submit" NAME="action_remove_arc" VALUE="oznaèit tuto zprávu ke smazání">
[ELSIF lang=hu]
  <INPUT TYPE="button" NAME="action_remove_arc" VALUE="A levél megjelõlése törlésre" onClick="request_confirm(this.form,'Tényleg törölni szeretnéd ezt a levelet?','remove_arc')">
[ELSIF lang=et]
