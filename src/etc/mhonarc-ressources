[ELSE]
  tag this mail for deletion
[ENDIF]
"></br>
  </FORM>
[ELSIF is_owner]
  <FORM ACTION="[path_cgi]" METHOD=POST>
  <INPUT TYPE="hidden" NAME="list" VALUE="[list]">
  <INPUT TYPE="hidden" NAME="yyyy" VALUE="$yyyy$">
  <INPUT TYPE="hidden" NAME="month" VALUE="$mois$">
