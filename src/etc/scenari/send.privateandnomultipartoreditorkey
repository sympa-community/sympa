is_editor([listname],[sender])                 smtp,md5,smime  -> do_it
match([header->Content-Type],/multipart/)      smtp,smime,md5  -> editorkey
is_subscriber([listname],[sender])             smtp,md5,smime  -> do_it
true()                                         smtp,smime,md5  -> editorkey
