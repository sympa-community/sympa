<!DOCTYPE html>
<html lang="[% lang %]" xml:lang="[% lang %]">
<!-- $Id$ -->
<head>
<meta charset="UTF-8" />
<meta name="generator" content="Sympa [% version %]" />

<meta name="viewport" content="width=device-width, initial-scale=1.0"> 

<title> [% main_title %] - [% action %] </title>

[% TRY; PROCESS head_ui.tt2; CATCH; ''; END %]

[% TRY; PROCESS head_javascript.tt2; CATCH; ''; END %]

[% IF base %] <base href="[% base %]" /> [% END %]

</head>

<body>

[% top_menu = 1 %]

<div id="top"><a class="displayNone" name="top" href="#content-inner">[%|loc%]Skip to Content.[%END%]</a></div>

[% IF top_menu %]
    [% TRY; PROCESS header.tt2; CATCH; ''; END %]
[% END %]

[% IF top_menu %][% IF not list %]<div id="loading" class="row">
[% ELSE %]<div id="loading" class="medium-9 columns">[% END %][% ELSE %]<div id="loading" class="medium-9 columns">[% END %]
   <div id="loadingText"><i class="fa fa-spinner fa-pulse"></i> <span></span></div></div>

    [% TRY; PROCESS aside_menu.tt2; CATCH; ''; END %]
    [% IF top_menu %]
      [%~ IF not list ~%]
        <main id="content-inner" class="row">
      [%~ ELSE ~%]
        <main id="content-inner" class="medium-9 columns">
      [%~ END %]
    [%~ ELSE ~%]
      <main id="content-inner" class="medium-9 columns">
    [%~ END ~%]
    <a name="content-inner"></a>

      [% IF not top_menu %]
        <h1>[% main_title %]</h1>
      [% END %]
      [% IF list %]
       [% TRY; PROCESS title.tt2; CATCH; ''; END %]
       [% IF list_title %]<p><b>[%|loc%]Subject:[%END%]</b> [% list_title %] [% IF is_owner %]<a href="[% path_cgi %]/edit_list_request/[% list %]/description"><i class="fa fa-pencil-square" title="[%|loc%](Edit)[%END%]"></i></a>[% END %]</p>[% END %]
      [% END %]

        [% TRY; PROCESS nav.tt2; CATCH; ''; END %]

<!--       <div class="ContentBlock">-->
         <h2>[%|loc%]Internal Server Error[%END%]</h2>
         <p>[%|loc%]Sympa encountered an internal error.[%END%]</p>
         <p>[%|loc%]Please contact the listmaster.[%END%]</p>

         [% IF 1 %][%# Change if you don't want to display traceback. %]
           <p>[%|loc(error_message)%]Error: %1[%END%]</p>

           <h2>[%|loc%]Traceback[%END%]</h2>

           <pre>[% traceback %]</pre>
         [% END %]
      <!--</div>--><!-- End div ContentBlock -->

[% IF top_menu %][% IF not list %][% END %][% END %]
</main><!-- End content-inner -->

<div class="displayNone"><a href="#top">[%|loc%]Top of Page[%END%]</a></div>

[% TRY; PROCESS footer.tt2; CATCH; ''; END %]

</body>

<!-- end tt2_error.tt2 -->
</html>
