  [% END -%]

[% END %]

[% IF owner_include %]
[% FOREACH o = owner_include -%]
owner_include
  source [% o.source %]
  
[% END -%]
[% END -%]

[% IF editor -%]
[% FOREACH e = editor -%]
editor
  email [% e.email %]
  [% IF e.gecos -%]
  gecos [% e.gecos %]
  [% END %]

[% END -%]
[% END %]

creation
  date [% creation.date %]
  date_epoch [% creation.date_epoch %]
[% IF creation_email -%]
  email [% creation_email %]
[% END %]

